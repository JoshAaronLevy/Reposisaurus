{
  "name": "repo-runner",
  "version": "0.1.0",
  "author": {
    "name": "Josh Levy"
  },
  "repository": {
    "url": "https://github.com/JoshAaronLevy/repo-runner"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "CI=true npm test --env=jsdom --coverage",
    "e2e": "cypress run --record=false",
    "e2e:open": "cypress open",
    "initiate": "npm install && npm run test && npm run build && npm start",
    "deploy": "rm -rf build && npm run build && firebase deploy && gitquick 'Deployed to Firebase'"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.2",
    "axios": "^0.26.0",
    "cypress-react-selector": "^2.3.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "primeflex": "^3.1.3",
    "primereact": "^7.2.1",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.2.3",
    "react-moment": "^1.1.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "testing-library/no-manual-cleanup": "warn"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.0-beta",
    "babel-jest": "^27.5.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^9.5.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-testing-library": "^5.0.6",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^0.6.5",
    "miragejs": "^0.1.43",
    "msw": "^0.38.2",
    "node-sass": "^7.0.1",
    "nyc": "^15.1.0",
    "react-test-renderer": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
